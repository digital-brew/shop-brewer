{
  "name": "digital-brew/shop-brewer",
  "description": "Add and remove WooCommerce options easy, with use of config file.",
  "keywords": ["themosis", "plugin"],
  "version": "1.0.0",
  "authors": [
    {
      "name": "Rafal Pajak",
      "email": "rafal@digitalbrew.io",
      "homepage": "https://digitalbrew.io"
    }
  ],
  "type": "wordpress-plugin",
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require": {
    "php": "^8.1",
    "composer/installers" : "^2.2"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
